{
  "verbose": true,
  "moduleFileExtensions": [
    "js",
    "ts"
  ],
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "tsConfig": "./tsconfig.json"
    }
  },
  "testMatch": [
    "**/test/*.+(ts)"
  ],
  "collectCoverage": true,
  "coverageReporters": [
    "json",
    "html"
  ],
  "preset": "ts-jest"
}
